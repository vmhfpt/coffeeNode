  <div class="banner header-text">
      <div class="owl-banner owl-carousel">
        {{#each dataBanner}}
            <div style="background-image: url('/img/banners/{{this.thumb}}');"  class='banner-item-0{{autoIndex @index}}'>
          <div class="text-content">
            <h4>{{this.name}}</h4>
            <h2>{{this.description}}</h2>
          </div>
        </div>
        {{/each}}
        
      </div>
    </div>


    <div class="latest-products">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="section-heading">
              <h2>Sản phẩm tốt</h2>
              <a href="products.html">Xem tất cả sản phẩm <i class="fa fa-angle-right"></i></a>
            </div>
          </div>
          
      {{#each dataProduct}}
            <div class="col-md-4">
            <div class="product-item">
              <a href="/{{this.slug}}"><img src="/img/products/{{this.thumb}}" alt=""></a>
              <div class="down-content">
                <a href="#"><h4>{{this.name}}</h4></a>
                <h6>{{this.price}}đ</h6>
                <p>{{this.description}}.</p>
                <ul class="stars">
                  <li><i class="fa fa-star"></i></li>
                  <li><i class="fa fa-star"></i></li>
                  <li><i class="fa fa-star"></i></li>
                  <li><i class="fa fa-star"></i></li>
                  <li><i class="fa fa-star"></i></li>
                </ul>
                <span>{{parentShow this.category_id}}</span>
              </div>
            </div>
          </div>
        {{/each}}
        </div>
      </div>
    </div>

